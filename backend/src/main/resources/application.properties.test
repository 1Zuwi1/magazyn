spring.datasource.url=jdbc:h2:mem:testdb
spring.datasource.driverClassName=org.h2.Driver
spring.datasource.username=sa
spring.datasource.password=password
spring.jpa.database-platform=org.hibernate.dialect.H2Dialect
spring.mail.host=localhost
spring.mail.port=1025
spring.mail.username=test
spring.mail.password=test
spring.mail.properties.mail.smtp.auth=false
spring.mail.properties.mail.smtp.starttls.enable=false
app.domain=localhost
app.name=GdzieToLezy
app.url=http://localhost:8080
app.webapp.url=http://localhost:3000
auth.password.min-length=8
spring.redis.host=redis
spring.redis.port=6379
spring.redis.password=${REDIS_PASSWORD}

app.s3.endpoint=http://localhost:9000
app.s3.region=us-east-1
app.s3.bucket=items-photos
app.s3.access-key=minioadmin
app.s3.secret-key=minioadmin

app.s3.items-prefix=items-photos/

webauthn.allow-untrusted-attestation:true

app.background-removal.enabled=true
app.background-removal.url=http://localhost:7000/api/remove
app.background-removal.timeout-seconds=30
